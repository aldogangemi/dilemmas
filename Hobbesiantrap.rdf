    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Distruzione -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Distruzione">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NonDistruzione"/>
        <rdfs:label xml:lang="en">Destruction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EconomicCriteria -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EconomicCriteria">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Ideologia"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#PoliticalPrinciples"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#SocialCriteria"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia"/>
        <rdfs:comment>E&apos; una classe di criteri basati su principi economici.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EndingOffensiveTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EndingOffensiveTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#EndingTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Has2Outcome"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Distruzione"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EndingPeaceTask"/>
        <rdfs:comment>Corrisponde alla task che finalizza un piano di attacco, offensivo militare</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EndingPeaceTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#EndingPeaceTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#EndingTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Has2Outcome"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NonDistruzione"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Corrisponde alla task che finalizza un piano di pace e cooperazione all&apos;interno di una situazione di tensione internazionale.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#HobbesianTrapDilematicSituations -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#HobbesianTrapDilematicSituations">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#DilemmaticSituation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl#satisfies"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ColdWar"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#MutualDistrustType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#Action"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask"/>
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrikeTask"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>La situazione dilemmatica Hobbesian Trap consiste nel non sapere chi farà la prima mossa, con due possibili outcomes legati a questa situazione: distruzione totale e reciproca oppure cooperazione e quindi resa da entrambe le parti, distensione.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Ideologia -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Ideologia">
        <owl:equivalentClass rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#PoliticalPrinciples"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#SocialCriteria"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia"/>
        <rdfs:comment>Il complesso delle idee e delle mentalità proprie di una società o di un gruppo sociale in un determinato periodo storico.</rdfs:comment>
        <rdfs:label xml:lang="en">Ideology</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#InformationTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#InformationTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AmpliamentoSferadinfluenza"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ResourceAllocationTask"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask"/>
        <rdfs:comment>task di controllo decisionale che si consegue valutando come gestire il flusso di informazioni per l&apos;avvio di possibili piani propagandistici.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#MutualDistrustType -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#MutualDistrustType">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ColdWar"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#HobbesianTrapDilematicSituations"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>MutualDistrust è una condizione di sfiducia reciproca, caratterizzata da l&apos;ansia costante e il sospetto verso le intenzioni e le azioni dell&apos;altra parte, le strategie di prevenire l&apos;aggressione attraverso la minaccia di una ritorsione
e la divisione ideologica</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NationalDefensiveTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NationalDefensiveTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#CaseTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ResourceAllocationTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Task eseguite per la difesa da attacchi esterni e per aumentare la sicurezza Nazionale.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NonDistruzione -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NonDistruzione">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
        <rdfs:label xml:lang="en">Not Destruction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#PoliticalPrinciples -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#PoliticalPrinciples">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#SocialCriteria"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia"/>
        <rdfs:comment>Il sistema concettuale e interpretativo che costituisce la base politica di un movimento, di un partito o di uno Stato.</rdfs:comment>
        <rdfs:label xml:lang="en">Political Principles</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ProStrike -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ProStrike">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask"/>
        <rdfs:comment>Evaluate the conditions and consequences of executing a first strike</rdfs:comment>
        <rdfs:label xml:lang="en">Pros</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ResourceAllocationTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ResourceAllocationTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask"/>
        <rdfs:comment>Task di controllo decisionale che si consegue valutando come gestire risorse logistiche per lo stanziamento di fondi e risorse militari.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#SocialCriteria -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#SocialCriteria">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia"/>
        <rdfs:comment>Criteri adottati dalla classe politica dirigente, tenendo conto delle esegienze sociali di quel Paese.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ControStrike"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ProStrike"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <rdfs:comment>Task decisionale di controllo richiesta dalla boolean case task &quot;strike strike&quot;, avente due sottoclassi disgiunte corrispondenti alla binarietà di una scelta booleana quale la finalizzazione o meno di una occasione di attacco.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrikeTask -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrikeTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#BooleanCaseTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#EndingTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#precede_solamente"/>
                <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategyTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Task che conseguono una scelta booleana sull&apos;occasione di attacco offensivo o meno.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Superpotenza -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Superpotenza">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Countries"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl#isDescribedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#Action"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Stato dotato di un grande apparato industriale e bellico, spec. in armamenti atomici.</rdfs:comment>
        <rdfs:label xml:lang="en">Superpowers</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremazia">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <rdfs:comment>Supremacy refers to the condition or state of being superior to others in authority, power, or status. It involves a hierarchical relationship where one group or entity is considered dominant and exerts control or influence over others. This dominance can be based on various factors, including but not limited to race, ethnicity, nationality, religion, or ideology.</rdfs:comment>
        <rdfs:label xml:lang="en">Supremacy</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AccumulazioneArmamenti -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AccumulazioneArmamenti">
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Negoziazione"/>
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#RiduzioneArmamenti"/>
        <participation:hasParticipant rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#URSS"/>
        <esegueiltask_striketask rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#FIrstStrike"/>
        <rdfs:label xml:lang="en">Arms Accumulation</rdfs:label>
    </owl:NamedIndividual>
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NegativePropertyAssertion"/>
        <owl:sourceIndividual rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AccumulazioneArmamenti"/>
        <owl:assertionProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#esegueiltask_distensivo"/>
        <owl:targetIndividual rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Distensione"/>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AlleanzaStrategica -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AlleanzaStrategica">
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#RiduzioneArmamenti"/>
        <participation:hasParticipant rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#URSS"/>
        <esegueiltask_influenza rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremacy"/>
        <rdfs:label xml:lang="en">Strategic Alliance</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AnalyzeEconomicRipercussions -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AnalyzeEconomicRipercussions">
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ConsultMilitaryAdvisors"/>
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Long-Term-Benefits"/>
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#RicercaMissilistica"/>
        <participation:hasParticipant rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#URSS"/>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Attackstrategyplanning -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Attackstrategyplanning">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ProStrike"/>
        <owl:differentFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#NotAttack"/>
        <taskrole:isTaskOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#MilitaryCommander"/>
        <taskrole:isTaskOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StrategicPlanner"/>
        <taskrole:isTaskOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#TechnologySpecialist"/>
        <eseguitomediante rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#ConsultMilitaryAdvisors"/>
        <eseguitomediante rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Long-Term-Benefits"/>
        <eseguitomediante rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#RicercaMissilistica"/>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AvvioPianiDiplomatici -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AvvioPianiDiplomatici">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#DiplomacyTask"/>
        <taskrole:isTaskOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#IntelligenceAnalyst"/>
        <è_compiuto_tramite rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#DecidereTeamNegozioazione"/>
        <è_compiuto_tramite rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Trattazioni"/>
        <rdfs:label xml:lang="en">Start Diplomatic Plans</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AvvioPianoPropaganda -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#AvvioPianoPropaganda">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#InformationTask"/>
        <sequence:directlyFollows rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#Supremacy"/>
        <eseguitomediante_propaganda rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#GatheringPublicInformations"/>
        <eseguitomediante_propaganda rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#StatisticResearch"/>
        <rdfs:label xml:lang="en">Strat Propagandistic Plan</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#BilancioMilitare -->

    <owl:NamedIndividual rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#BilancioMilitare">
        <participation:hasParticipant rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/hobbesiantrap.owl#URSS"/>
