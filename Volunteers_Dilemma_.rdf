                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choosing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


