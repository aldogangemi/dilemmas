    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Benefit -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Benefit">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#exists_if"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#SomeoneVolunteer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">The benefit that the action will provide to every member of the group. An example is when someone
 needs to pay 0.20$ in order to obtain 1$ for every member of the group. The benefit will be 1$.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choice -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choice">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
        <dc:creator>Aldo Gangemi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-05-07T16:06:26Z</dc:date>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#ChooseToVolunteer -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#ChooseToVolunteer">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choosing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">The choice of the single individual to volunteer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choosing -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choosing">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#BooleanCaseTask"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#ChooseToVolunteer"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotChooseToVolunteer"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A boolean case task that consists in choosing whether to volunteer or not. It starts a deliberation task:&quot;ChooseToVolunteer&quot; or &quot;NotChooseToVolunteer&quot;.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#CostBenefitAnalysis -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#CostBenefitAnalysis">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Criterion"/>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">Every individual in the group can make a decision based upon a personal cost-benefit analysis.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#CostOfAction -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#CostOfAction">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#exists_if"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#SomeoneVolunteer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">The action that provides the benefit for the group is associated to a cost, tipically a little one compared to the benefit. For example, in a scenario where someone needs to pay 0.20$ in order to obtain 1$ for every member of the group, the Cost of the action is 0.20$.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Criterion -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Criterion">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
        <dc:creator>Aldo Gangemi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-05-07T16:28:25Z</dc:date>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Dilemma -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Dilemma">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#usesConcept"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#TaskDecision"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Aldo Gangemi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-05-07T14:57:29Z</dc:date>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#DilemmaticSituation -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#DilemmaticSituation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl#satisfies"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Dilemma"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#Action"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask"/>
                                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                                <owl:onClass rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Task"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Aldo Gangemi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-05-07T15:07:29Z</dc:date>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#GenerousApproach -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#GenerousApproach">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Criterion"/>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">Someone is by nature more caregiving than others. In a larger group is possible that more people will be more generous and willing to pay the cost of the action, in order to give something to the group. This can explane why in the study of this dilemma, theory doesn&apos;t match practice.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Group -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Group">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#receive"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Benefit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">A group of people where at least one volunteer is needed to provide a benefit for everyone.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NoOneVolunteer -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NoOneVolunteer">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Outcome"/>

        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">If there is no volunteer no one receives the benefit</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotChooseToVolunteer -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotChooseToVolunteer">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Choosing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">The choice of the single individual not to volunteer</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotVolunteer -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotVolunteer">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Role"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#exists_if"/>
                        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NoOneVolunteer"/>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">The individual who chooses not to volunteer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#ObtainBenefit -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#ObtainBenefit">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#VolunteerTask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#PayingCost"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#isTaskOf"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotVolunteer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#isTaskOf"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Volunteer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">After the cost is paid, everyone in the group obtains the benefit.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Other -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Other">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Group"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#NotVolunteer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
