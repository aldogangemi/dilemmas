        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#Outcome"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/description.owl#usesConcept"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#causallyLinkedTo"/>
        </owl:propertyChainAxiom>
        <dc:creator>Aldo Gangemi</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-05-07T15:03:06Z</dc:date>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#receive -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#receive">
        <dc:creator>Francesco Bergamaschi</dc:creator>
        <rdfs:comment xml:lang="en">A relation between two objects in which one  of them recieves the other one. For example, if I buy an apple, on one hand I receive an apple and, on the other hand, the seller receives money.</rdfs:comment>
    </owl:ObjectProperty>
    


