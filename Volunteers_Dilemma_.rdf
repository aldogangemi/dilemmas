            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#receive"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/volunteersdilemma.owl#CostOfAction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DecisionActivity -->

