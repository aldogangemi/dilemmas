    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Temperature -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Temperature">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DecisionActivity"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>Each hedgehog has its own temperature. 
If it is &gt;20, the hedgehog will search contact with other animals to rise its temperature.</rdfs:comment>
    </owl:DatatypeProperty>
    


