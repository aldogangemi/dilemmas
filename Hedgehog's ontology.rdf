     -->

    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#hasOption -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#hasOption">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Task"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <dc:creator>Walter Bertoli</dc:creator>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#hasCriterion -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#hasCriterion">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/description.owl#Description"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <dc:creator>Walter Bertoli</dc:creator>
    </owl:ObjectProperty>
    


