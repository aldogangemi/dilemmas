    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#hasOutcome"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome -->

    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#hasCriterion"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetCloser -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetCloser">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveAway"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#ProperDistance"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#HedgehogSituation -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#HedgehogSituation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#DilemmaticSituation"/>
        <dc:creator>Walter Bertoli</dc:creator>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#HedgehogTask -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#HedgehogTask">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Task"/>
        <dc:creator>Walter Bertoli</dc:creator>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Hedgehogs -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Hedgehogs">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Humans"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent"/>
        <dc:creator>Walter Bertoli</dc:creator>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Humans -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Humans">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>The Hedgehog Dilemma is a metaphora for the human&apos;s social relations</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Individualism -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Individualism">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Interdependence"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Kindness"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Interdependence -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Interdependence">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Kindness"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Kindness -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Kindness">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Criterion"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveAway -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveAway">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#ProperDistance"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#ProperDistance -->

    <owl:Class rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#ProperDistance">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dilemmas/coredilemma.owl#Outcome"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ChooseMoralValue -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ChooseMoralValue">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#CaseTask"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Evaluate_Conditions"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#EvaluateOptions"/>
        <Hedgehog_Dilemma:hasOption rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Individualism"/>
        <Hedgehog_Dilemma:hasOption rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Interdependence"/>
        <Hedgehog_Dilemma:hasOption rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Kindness"/>
        <rdfs:comment>The Hedgehog decide which criterion will drive its decisions.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#EvaluateOptions -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#EvaluateOptions">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationTask"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Evaluate_Conditions"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ChooseMoralValue"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions"/>
        <rdfs:comment>Once the moral values have been chosen, the Hedgehog analyzes and plans the situation, evaluateing the distance it wants to take.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Evaluate_Conditions -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Evaluate_Conditions">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#ActivationTask"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#BeingAlone_and_Pain"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ChooseMoralValue"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>Being in a Pain and Cold state (the DecisionState), the Hedgehog begins to evaluate the phisical external conditions.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#ConcurrencyTask"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#EvaluateOptions"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#StabilizePosition"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>The Hedgehog explores simultaneously all of the options, considering wich movement to take.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#FeelGood -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#FeelGood">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#EndingTask"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement"/>
        <rdfs:comment>To complete the dilemma, the Hedgehog has to feel good, applying his choices and values in order to find its place in the ralations with others</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog1">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role"/>
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Role"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog2"/>
        <descriptionandsituation:isDescribedBy rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Individualism"/>
        <taskrole:hasTask rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog2 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog2">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role"/>
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/taskrole.owl#Role"/>
        <descriptionandsituation:isDescribedBy rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Interdependence"/>
        <taskrole:hasTask rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#ActionTask"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#StabilizePosition"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#FeelGood"/>
        <taskrole:isTaskOf rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog1"/>
        <taskrole:isTaskOf rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Hedgehog2"/>
        <coredilemma:causallyLinkedTo rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetCloser"/>
        <coredilemma:causallyLinkedTo rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveAway"/>
        <coredilemma:causallyLinkedTo rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#ProperDistance"/>
        <rdfs:comment>In order to realize its decision the Hedgehog has to implement movement. Here the animal sequences a non-planning activity, which is an automatic activity that doesn&apos;t involve a rational event.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#StabilizePosition -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#StabilizePosition">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#SynchroTask"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#ExploreOptions"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Movement"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>In the synchro task the Hedgehog gets imputs from the Concurrency task, coordinateing the various actions to get the right balance between pain and heat according to the decision made and the criterion used</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#BeingAlone_and_Pain -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#BeingAlone_and_Pain">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DeliberationState"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Coldness"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetHurt"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveCloser"/>
        <sequence:follows rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetHurt"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma#Evaluate_Conditions"/>
        <dc:creator>Walter Bertoli</dc:creator>
        <rdfs:comment>The Hedgehog, after it has experienced coldness and pain, is in a state between being alone and mental pain. Now it is in the situation to choose what to do.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Coldness -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#Coldness">
        <rdf:type rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/controlflow.owl#DecisionActivity"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#GetHurt"/>
        <owl:differentFrom rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveCloser"/>
        <sequence:precedes rdf:resource="http://www.semanticweb.org/walte/ontologies/2024/5/Hedgehog_Dilemma9#MoveCloser"/>
